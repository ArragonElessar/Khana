<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('./partials/head.ejs')%>
        <script type="text/javascript">
            var key = '<%=key%>'
        </script>
        <script src="js/index.js" type="text/javascript"></script>
</head>

<body style="background: var(--bs-gray);">
    <header>
        <script type="text/javascript" src='js/navbar.js'></script>
        <%- include('./partials/nav.ejs') %>
    </header>
    <main class="text-center">
        <section class="clean-block slider">
            <div class="container">
                <div class="carousel slide" data-bs-ride="carousel" id="carousel-1">
                    <div class="carousel-inner">
                        <div class="carousel-item active">
                            <img class="w-100 d-block" src="img/img1.png" name=carousel-image
                                alt="Slide Image" loading="auto"></div>
                        <div class="carousel-item">
                            <img class="w-100 d-block" src="img/img2.png" alt="Slide Image" name=carousel-image>
                        </div>
                        <div class="carousel-item">
                            <img class="w-100 d-block" src="img/img3.png" alt="Slide Image" name=carousel-image>
                        </div>
                        <div class="carousel-item">
                            <img class="w-100 d-block" src="img/menu1.png" alt="Slide Image" name=carousel-image>
                        </div>
                    </div>
                    <div><a class="carousel-control-prev" href="#carousel-1" role="button" data-bs-slide="prev">
                            <span class="carousel-control-prev-icon"></span>
                            <span class="visually-hidden">Previous</span>
                        </a>
                        <a class="carousel-control-next" href="#carousel-1" role="button" data-bs-slide="next">
                            <span class="carousel-control-next-icon"></span>
                            <span class="visually-hidden">Next</span>
                        </a>
                    </div>
                    <ol class="carousel-indicators">
                        <li data-bs-target="#carousel-1" data-bs-slide-to="0" class="active"></li>
                        <li data-bs-target="#carousel-1" data-bs-slide-to="1"></li>
                        <li data-bs-target="#carousel-1" data-bs-slide-to="2"></li>
                        <li data-bs-target="#carousel-1" data-bs-slide-to="3"></li>
                    </ol>
                </div>
            </div>
        </section>
    </main>
    <%- include('./partials/footer.ejs') %>


</body>

</html>